#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -x "$basedir/node" ]; then
  "$basedir/node"  "$basedir/../_promises-aplus-tests@2.1.2@promises-aplus-tests/lib/cli.js" "$@"
  ret=$?
else 
  node  "$basedir/../_promises-aplus-tests@2.1.2@promises-aplus-tests/lib/cli.js" "$@"
  ret=$?
fi
exit $ret
